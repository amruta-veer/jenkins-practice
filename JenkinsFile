pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Checking out code...'
            }
        }

        stage('Install') {
            steps {
                echo 'Installing dependencies...'
                bat 'pip install pandas numpy'
            }
        }

        stage('Test') {
            steps {
                echo 'Running tests...'
                bat 'python -c "import pandas; print(pandas.__version__)"'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying...'
                echo 'Pipeline complete!'
            }
        }
    }
}
